{
  "name": "environmental_monitoring_platform",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "build/index.js",
  "pkg": {
    "assets": [
      "static/**/*"
    ]
  },
  "scripts": {
    "prestart": "nvm use 16.13.2",
    "start": "nodemon --exec npx babel-node server/index.js",
    "dev": "babel-watch server/index.js",
    "build:pkg": "rm -rf ./build/* && babel server --out-dir build && pkg . --target host --output platform",
    "build:prod": "babel server --out-dir build",
    "docker:build": "docker build -t app_service:v1 .",
    "docker:start_dev": "docker run -it -p 4000:4000 --restart unless-stopped -e NODE_ENV=development --name app_service app_service:v1",
    "docker:start_prod": "docker run -it -p 4000:4000 --restart unless-stopped -e NODE_ENV=production --name app_service app_service:v1"
  },
  "author": "vietbq@centic.vn",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.8.4",
